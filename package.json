{
  "name": "cuuursors.com",
  "version": "1.0.0",
  "description": "Website for cuuursors.com",
  "main": "index.js",
  "repository": "https://github.com/jaredpike/cuuursors.com",
  "author": "Jared Pike",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^1.2.0",
    "copy-webpack-plugin": "^4.2.3",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "gh-pages": "^1.1.0",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.5",
    "postcss-custom-media": "^6.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-media-minmax": "^3.0.0",
    "postcss-urlrev": "^2.0.0",
    "sass-loader": "^6.0.6",
    "scrollmonitor": "^1.2.4",
    "style-loader": "^0.19.0",
    "takeshape-cli": "^4.61.0",
    "three": "^0.98.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4",
    "webpack-manifest-plugin": "^1.3.2"
  },
  "scripts": {
    "init": "tsg init",
    "clean": "rm -rf static/assets/javascripts && rm -rf static/assets/stylesheets && rm -rf static/assets/images && rm -f static/assets/*.json",
    "serve": "node -e 'setTimeout(function() {}, 2000)' && browser-sync start --server build --port 5000 --files 'build/**/*, !build/sitemap.xml, !build/feed.xml' --reload-debounce",
    "deploy": "npm run build && tsg deploy",
    "build:webpack": "NODE_ENV=production webpack --display-error-details -p",
    "build:webpack:dev": "webpack --display-error-details -d",
    "build:tsg": "tsg build",
    "build": "npm-run-all --serial clean build:webpack",
    "watch:js": "webpack --watch",
    "watch:tsg": "tsg watch -W",
    "watch": "npm-run-all --serial clean build:webpack:dev build:tsg && npm-run-all --continue-on-error --parallel watch:* serve",
    "start": "npm run watch"
  }
}
